{
  "name": "@ppb/orm",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./generated/ppb": {
      "types": "./generated/ppb/index.d.ts",
      "default": "./generated/ppb/index.js"
    }
  },
  "scripts": {
    "test": "jest",
    "copy-generated:esm": "cp -r src/generated dist/esm/",
    "copy-generated:cjs": "cp -r src/generated dist/cjs/",
    "build:cjs": "tsc -p tsconfig.cjs.json && pnpm run copy-generated:cjs",
    "build:esm": "tsc -p tsconfig.esm.json && pnpm run copy-generated:esm",
    "build": "rm -rf dist && pnpm db:generate && pnpm run build:cjs && pnpm run build:esm",
    "db:pull:ppb": "prisma db pull --schema=prisma/ppb/schema.prisma",
    "db:generate:ppb": "prisma generate --schema=prisma/ppb/schema.prisma",
    "db:generate": "pnpm run db:generate:ppb"
  },
  "keywords": ["ppb", "orm"],
  "files": ["dist", "generated"],
  "author": "jiwoniy@guardianslab.com",
  "license": "ISC",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/engines",
      "prisma"
    ]
  },
  "devDependencies": {
    "prisma": "^6.15.0",
    "@ppb/config": "workspace:*"
  },
  "dependencies": {
    "@prisma/client": "6.15.0"
  }
}
